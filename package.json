{
  "name": "react-tempusdominus-bootstrap",
  "version": "2.0.0-a",
  "description": "A React wrapper for the Tempus Dominus Bootstrap plugin for date and time pickers.",
  "author": "Anton Bagdatyev (Tonix)",
  "license": "MIT",
  "repository": "tonix-tuft/react-tempusdominus-bootstrap",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "keywords": [
    "react",
    "react-component",
    "component",
    "bootstrap",
    "datepicker",
    "datetimepicker",
    "timepicker"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "mkdir-build": "npx mkdirp dist",
    "prepare": "npm run mkdir-build && npm run clear-build && npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "clear-build": "rimraf dist",
    "publish-patch": "npm run prepare && npm version patch && npm publish && git push && git push --tags",
    "publish-minor": "npm run prepare && npm version minor && npm publish && git push && git push --tags",
    "publish-major": "npm run prepare && npm version major && npm publish && git push && git push --tags"
  },
  "peerDependencies": {
